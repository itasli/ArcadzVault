{
  "name": "arcadzvault",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "npx hardhat test",
    "test:coverage": "SOLIDITY_COVERAGE=true npx hardhat coverage",
    "build": "hardhat compile && npm run codegen",
    "codegen": "wagmi generate",

    "node" : "npx hardhat node",
    "deploy:localhost": "npx hardhat ignition deploy ./ignition/modules/ArcadzVault.ts --network localhost",
    "deplot:testnet": "npx hardhat ignition deploy ./ignition/modules/ArcadzVault.ts --network fuji",
    "deploy:mainnet": "npx hardhat ignition deploy ./ignition/modules/ArcadzVault.ts --network mainnet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itasli/ArcadzVault.git"
  },
  "author": "itasli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itasli/ArcadzVault.git/issues"
  },
  "homepage": "https://github.com/itasli/ArcadzVault#readme",
  "description": "",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox-viem": "^3.0.0",
    "@openzeppelin/contracts": "^5.3.0",
    "@wagmi/cli": "^2.3.1",
    "hardhat": "^2.24.3"
  }
}
